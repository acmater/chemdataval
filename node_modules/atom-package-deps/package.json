{
  "name": "atom-package-deps",
  "version": "6.0.0",
  "description": "Automatically install package dependencies",
  "main": "lib/index.js",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "lint": "(eslint .) && (flow check) && (prettier --list-different src/*.js)",
    "test": "apm test",
    "clean": "rm -rf lib",
    "dev": "npm run clean && env NODE_ENV=development env BABEL_ENV=production rollup -c -w",
    "build": "npm run clean && env NODE_ENV=production env BABEL_ENV=production rollup -c",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/steelbrain/package-deps.git"
  },
  "keywords": [
    "atom"
  ],
  "author": "steelbrain",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AtomLinter/package-deps/issues"
  },
  "homepage": "https://github.com/AtomLinter/package-deps#readme",
  "dependencies": {
    "sb-fs": "^4.0.0",
    "semver": "^7.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-flow": "^7.10.1",
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-node-resolve": "^8.0.1",
    "@types/atom": "^1.40.4",
    "@types/jasmine": "^3.5.10",
    "@types/semver": "^7.2.0",
    "eslint-config-steelbrain": "^9.0.1",
    "flow-bin": "^0.126.1",
    "jasmine-fix": "^1.3.1",
    "prettier": "^2.0.5",
    "rollup": "^2.14.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-terser": "^6.1.0",
    "typescript": "^3.9.5"
  }
}
